{
	"name": "chat helper",
	"version": "0.1",
	"manifest_version": 2,
	"description": "",
	"content_scripts": [{
		"matches": ["*://chat.google.com/*", "*://mail.google.com/chat/*"],
		"all_frames": true,
		"js": [ "content_scripts/insQ.min.js", "content_scripts/content.js" ],
		"css": [ "content_scripts/style.css" ]
	}],
	"background": {
		"scripts": []
	},
	"browser_action": {
		"default_popup": "browser_action/popup.html"
	},
	"options_page": "options_page/options.html",
	"permissions": [
		"storage"
	]
}
